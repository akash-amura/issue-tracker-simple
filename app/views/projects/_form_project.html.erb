<%= form_for @project,url:projects_path do |f| %>
  <p>
  <%=  f.label :title %>
  <%= f.text_field :title %>
  </p>
  <p>
  <%= f.label :description %>
  <%= f.text_area :description %>
  </p>
  <p>
  <%= f.label :planned_completion_date %> 
  <%= f.date_field :planned_completion_date %>
  </p>
  <p>
  <%= f.label :manager_id %>
  <%= f.text_field :manager_id %>
  </p>
  <p>
  <%= f.label :creator_id %> 
  <%= f.text_field :creator_id %>
  </p>
  <p>
  <%= f.submit %>
  </p>
<%  end %>
