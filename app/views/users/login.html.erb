<h1>User#Login</h1>
<%= render "partials/flash_message" %>
<%=  form_tag  do %>
  <p>
  <%=  label_tag :email %>
  <%=  text_field_tag :email %> 
  </p>


  <p>  
  <%=  label_tag :password %>
  <%=  password_field_tag :password %>
  </p>

  <p>
    <span><%= check_box_tag 'remember' %></span>
    <span><%= label_tag :remember_me %></span>
  </p>
  
  <p>
  <%=  submit_tag :login %>
  </p>
<%  end %>

or
<%= link_to 'Signup',new_user_path %>
